	<select id="selectRolesOrganTree" resultMap="TreeResultMap"
		parameterType="java.lang.String">
		select * from
		(SELECT ID,ORGAN_NAME AS TEXT,PARENT_ID AS PARENTID ,'0' AS ISLEAF,ID AS
		ORGANID
		FROM SYS_ORGAN
		WHERE DELETED = '0' AND tier_code LIKE #{tierCode,jdbcType=VARCHAR} ) org
		union
		SELECT R.ID,ROLE_NAME AS TEXT,ORGAN_ID AS PARENTID ,'1' AS ISLEAF,ORGAN_ID
		AS ORGANID
		FROM SYS_ROLE R,(SELECT ID,ORGAN_NAME AS TEXT,PARENT_ID AS PARENTID ,'0'
		AS ISLEAF,ID AS ORGANID
		FROM SYS_ORGAN
		WHERE DELETED = '0' AND tier_code LIKE #{tierCode,jdbcType=VARCHAR} ) ORG
		WHERE ORGAN_ID = ORG.ORGANID
	</select>

