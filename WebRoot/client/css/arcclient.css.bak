@keyframes arcClickedAnimation {
	50% { transform: translate(1px) scale(0.99)}
	100% { transform: translate(0px) scale(1)}
}
@-moz-keyframes arcClickedAnimation {
	50% { -moz-transform: translate(1px) scale(0.99)}
	100% { -moz-transform: translate(0px) scale(1)}
}
@-webkit-keyframes arcClickedAnimation {
	50% { -webkit-transform: translate(1px) scale(0.99) }
	100% { -webkit-transform: translate(0px) scale(1) }
}
.arcClicked
{
	-webkit-animation: arcClickedAnimation 0.5s 1 ease-in-out;
	-moz-animation: arcClickedAnimation 0.5s 1 ease-in-out;
	animation: arcClickedAnimation 0.5s 1 ease-in-out;
	background-color: #F0F0F0 !important;
	color: #666666 !important;
	opacity: 0.5 !important;
	-moz-box-shadow: 0 0 6px #888888, 0 0 10px #888888 inset;
	-webkit-box-shadow: 0 0 6px #888888, 0 0 10px #888888 inset;
	box-shadow: 0 0 6px #888888, 0 0 10px #888888 inset;
	visibility: visible !important;
}
input.arcClicked
{
	opacity: 0 !important;
}
input.arcClicked + label > span::before
{
	-webkit-animation: arcClickedAnimation 0.5s 1 ease-in-out;
	-moz-animation: arcClickedAnimation 0.5s 1 ease-in-out;
	animation: arcClickedAnimation 0.5s 1 ease-in-out;
	-moz-box-shadow: 0 0 6px #888888, 0 0 10px #888888 inset;
	-webkit-box-shadow: 0 0 6px #888888, 0 0 10px #888888 inset;
