	<select id="getRolesByUser" parameterType="string" resultMap="BaseResultMap">
		SELECT
		r.*
		FROM
		sys_role r
		JOIN
		sys_user_role ur
		ON
		r.id = ur.role_id
		WHERE
		ur.user_id = #{userId,jdbcType=VARCHAR}
	</select>

