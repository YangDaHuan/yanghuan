	<select id="findRoleByNameAndOrganId" resultMap="BaseResultMap">
		select * from sys_role
		<where>
			<if test="roleName!=null and roleName!=''">
				AND ROLE_NAME = #{roleName,jdbcType=OTHER}
			</if>
			<if test="organId!=null and organId!=''">
				AND ORGAN_ID = #{organId,jdbcType=VARCHAR}
			</if>
		</where>
	</select>
	<select id="getRoleListByModule" resultMap="BaseResultMap">
		SELECT T0.ROLE_ID AS ID
		FROM SYS_ROLE_MODULE T0
		<foreach collection="funs" index="index" item="fun" separator="">
			,SYS_ROLE_MODULE T${index+1}
		</foreach>

		WHERE T0.MODULE_ID=#{moduleId,jdbcType=VARCHAR}
		AND (T0.FUN_CODE IS NULL AND T0.FUN_VALUE IS NULL)
		<foreach collection="funs" index="index" item="fun" separator="">
			AND (T0.MODULE_ID=T${index+1}.MODULE_ID AND
			T0.ROLE_ID=T${index+1}.ROLE_ID
			AND T${index+1}.FUN_CODE =#{fun[0]} AND T${index+1}.FUN_VALUE
			=#{fun[1]})
		</foreach>

	</select>

