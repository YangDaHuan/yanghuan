package com.solidextend.sys.authorize.vo;

public class Authorize {

	/** 角色ID  */
	private String roleId;
	/** 可用模块授权集合 ID逗号分隔 如 11,12,13 */
	private String useModules;
	/** 可分配模块授权集合 ID逗号分隔 如 11,12,13 */
	private String assignModules;
	/** 可用功能授权集合form字符串逗号分隔 如 grant=1,grant=0&print=1,grand=0 */
	private String funForms;
	
	public String getRoleId() {
		return roleId;
	}
	public void setRoleId(String roleId) {
		this.roleId = roleId;
	}
	public String getUseModules() {
		return useModules;
	}
	public void setUseModules(String useModules) {
		this.useModules = useModules;
	}
	public String getAssignModules() {
		return assignModules;
	}
	public void setAssignModules(String assignModules) {
		this.assignModules = assignModules;
	}
	public String getFunForms() {
		return funForms;
	}
	public void setFunForms(String funForms) {
		this.funForms = funForms;
	}
	
	
}
