<style type="text/css">
html, body {
	height: auto;
}
</style>
<script type="text/javascript">
$(function(){
	$('#tt').tree({
		url: 'tree_data_module.json',
		parentField:"pid",
		onClick:function(node){
			//alert(node.attributes.url);
		}
	});
	
