.arcHier li > input
{
	position: absolute;
	opacity: 0;
	z-index: 1;
}
.arcHier li > input:not(:disabled)
{
	cursor: pointer;
}
.arcHier ul,
.arcHier li
{
	padding: 0;
	margin: 0;
	list-style-type: none;
	overflow: hidden;
	white-space: nowrap;
}
.arcHier ol
{
	display: inline-block;
	list-style-type: none;
	-webkit-padding-start: 0px; 
	padding: 0;
	margin: 0;
	vertical-align: top;
}
.arcHier label > span::before
{
	margin-top: -2px;
	margin-left: -16px;
	width: 16px;
	height: 10px;
	content: "";
	background-color: transparent;
	white-space: nowrap;
	vertical-align: top;
	display: inline-block;
	position: relative;
}
.arcHier input:checked + label > span::before
{
	background-image: url(../img/minus.png);
	background-repeat: no-repeat;
	background-position: 0px center;
}
.arcHier input:not(checked) + label > span::before
{
	background-image: url(../img/plus.png);
	background-repeat: no-repeat;
	background-position: 0px center;
}
.arcHier div {
	padding: 0;
	margin: 0;
	width: 100%;
	height: 100%;
}
.arcHier label
{
	white-space: nowrap;
	vertical-align: top;
}
.arcHier a
{
	color: inherit;
	text-decoration: none;
	white-space: nowrap;
	overflow: visible;
	display: inline-block;
	vertical-align: top;
}
.arcHier label::before
{
	content: "";
	width: 16px;
	overflow: visible;
	display: inline-block;
	white-space: nowrap;
}
.arcHier span
{
	margin-right: 0px;
	margin-bottom: 0px;
	width: 0px;
	height: 7px;
	vertical-align: top;
	background-color: #ffff00;
	border-color: #000000;
	border-width: 0px;
	border-style: solid;
	display: inline-block;
	-moz-user-select: none;
	-webkit-user-select: none;
	-webkit-touch-callout: none;
	resize: none;
	z-index: 0;
}
.arcHier ol > li ol li span
{
	background-color: #00ff00;
}
.arcHier ol > li ol ol li span
{
	background-color: #0000ff;
}
.arcHier ol > li ol ol ol li span
{
	background-color: #ff0000;
}
div.arcVert ul,
div.arcVert ol
{
	width: 100%;
}
div.arcVert ul > li,
div.arcVert ol > li
{
	padding-left: 3px;
	width: 100%;
}
.arcVert input + label + ul,
.arcVert input + label + ol + ul
{
	margin: 0 0 0 9px; 
}
.arcVert input ~ ul
{
	display: none;
}
.arcVertNoExp input ~ ul 
{
    display: block;
}
.arcVert a + ul,
.arcVert a + ol + ul
{
	margin: 0 0 0 9px; 
}
.arcVert input:checked ~ ul
{
	display: block;
}
