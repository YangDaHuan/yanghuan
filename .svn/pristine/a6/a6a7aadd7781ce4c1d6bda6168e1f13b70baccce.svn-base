}
.module {
    cursor: pointer;
    display: block;
    height: 48px;
    margin: 0 auto;
    width: 48px;
}
.cb {
    clear: both;
}
/*导航菜单*/
.tabBar{
	overflow: hidden;
	position: relative;
	height: 72px;
}
.tabBarBg{
	border-width: 0 0 1px 0;
	border-color: #cdd1d4;
	border-style: solid;
	background: url("../images/maintab_bg.png") repeat-x scroll 0 0 #EAEFF2;
	margin: 0;
}
.tabBody{
	position: relative;
	border-top: 1px solid #e0e5e8;
	background-color:#eaeff2;
}
.tabMain{
	clear: both;	
}
.tab_icon{
	width: 16px;height: 16px;
	position: absolute;top: 5px;left:2px;
	z-index: 2;
	background-repeat: no-repeat;
}

/* tab */
.tabs-nav {
	position:absolute;
	left:0;
	bottom:5px;
	width:3000px;
	height:39px;
    font-family: "Trebuchet MS", Trebuchet, Verdana, Helvetica, Arial, sans-serif;
    list-style: none;
    margin: 0;
    border-bottom: #fff 1px solid;
}
.tabs-nav:after { 
    display: block;
    clear: both;
    content: " ";
}
.tabs-nav li {
    float: left;
    padding: 0 0 0 7px;
    min-width: 84px; /* be nice to Opera */
    position:relative;
}

.tabs-nav a, .tabs-nav a span {
    display: block;
    padding: 0 10px;
    background: url(../images/tab.png) no-repeat;
    line-height: 31px;    
}
.tabs-nav a {
    margin: 1px 0 0; 
    padding-left: 0;
    color: #9eb8d8;
    font-weight: bold;
    line-height: 1.2;
    text-align: center;
    text-decoration: none;
    white-space: nowrap;
    outline: 0; 
    height: 38px;
}
.tabs-nav .active a {
    position: relative;
    top: 2px;
    z-index: 2;
    margin-top: 0;
    color: #5a6e7d;
    cursor: default;
}
.tabs-nav a span {
    width: 64px; 
    min-width: 64px;
    height: 18px; 
    min-height: 18px;
    padding:6px 6px 0 18px;
    text-align: center;
    cursor: pointer;
}
*>.tabs-nav a span { 
    width: auto;
    height: auto;
}
.tabs-nav .active a span {
    padding-bottom: 1px;
    cursor: default;
}
.tabs-nav a:hover, .tabs-nav a:focus, .tabs-nav a:active {
    background-position: 100% -120px;
}
.tabs-nav .active a{
	background-position: 100% -200px;
}
.tabs-nav a, .tabs-nav .ui-tabs-disabled a:hover, .tabs-nav .ui-tabs-disabled a:focus, .tabs-nav .ui-tabs-disabled a:active {
    background-position: 100% -40px;
}

.tabs-nav a:hover span, .tabs-nav a:focus span{
    background-position: 0 -80px;
}
.tabs-nav a span, .tabs-nav .ui-tabs-disabled a:hover span, .tabs-nav .ui-tabs-disabled a:focus span, .tabs-nav .ui-tabs-disabled a:active span {
    background-position: 0 0;
}
.tabs-nav .active a:link, .tabs-nav .active a:visited, .tabs-nav .ui-tabs-disabled a:link, .tabs-nav .ui-tabs-disabled a:visited { 
    cursor: text;
}
.tabs-nav a:hover, .tabs-nav a:focus, .tabs-nav a:active { 
    cursor: pointer;
}
.tabs-nav .active a span{
	background-position: 0 -160px;
}
* html .tabs-nav { 
    display: inline-block;
}
*:first-child+html .tabs-nav  { 
    display: inline-block;
}
.tab-body-child{
	height: 100%;
}