<script>
	function singleChecked(target){
		var name=$(target).attr("name");
		if(name==null||name.length==0)return;
		var value=$(target).val();
		var checkboxs=$("input[name='"+name+"']");
		for(var i=0;i<checkboxs.length;i++){
			if($(checkboxs[i]).val()==value){
				$(checkboxs[i]).attr("checked", true);
			}else{
				$(checkboxs[i]).attr("checked", false);
			}
		}
	}
	function getCheckBoxValues(name){
		if(name==null||name.length==0)return;
		var values=new Array();
		var checkboxs=$("input[name='"+name+"']");
		for(var i=0;i<checkboxs.length;i++){
			if($(checkboxs[i]).attr("checked")){
				values[values.length]=$(checkboxs[i]).val();
			}
		}
		return values;
	}
	</script>
